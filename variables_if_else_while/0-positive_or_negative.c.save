#include <stdlib.h>
#include <time.h>
/**
*main - Entry point
*Return: 
*/
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;

	if (n < 0)
	{
		printf("%d is negatif\n", n);
	}
	else if (n == 0)
	{
		printf("%d is zero\n", n);
	}
	else
	{
		printf("%d is positif\n", n);
	}
	return (0);
}
